<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.oop_project.Cart">
    <ScrollPane maxWidth="600.0" prefHeight="334.0" prefWidth="600.0">
        <VBox fx:id="quantifiables" maxWidth="600.0" />
    </ScrollPane>
        <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0">
            <GridPane prefHeight="66.0" prefWidth="600.0">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints vgrow="SOMETIMES" />
                </rowConstraints>
                <Label fx:id="totalText" text="Total: ">
                    <font>
                        <Font size="38.0" />
                    </font>
                </Label>
                <Button mnemonicParsing="false" prefHeight="72.0" prefWidth="299.0" text="Check Out" GridPane.columnIndex="1" onAction="#onCheckClick">
                    <font>
                        <Font size="24.0" />
                    </font>
                </Button>
            </GridPane>
        </AnchorPane>
</VBox>
